{
  "compilerOptions": {
    "composite": true,
    "target": "es2020",
    "module": "commonjs",
    "removeComments": true,
    "strict": true,
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "./services",
    "sourceMap": true
  }
}
